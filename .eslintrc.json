{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
   "max-classes-per-file": [
      "error",
      { "ignoreExpressions": true, "max": 2 }
  ],
    "no-param-reassign": 0,
    "import/extensions": 0,
    "no-console": 0
  }
}
